<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <jahia-academy-template j:dependencies="bootstrap3-components bootstrap3-core default jquery toc highlightjs"
                              j:installedModules="jahia-academy-template"
                              j:modulePriority="0"
                              j:moduleType="templatesSet"
                              j:title="Jahia Academy Template"
                              jcr:primaryType="jnt:module">
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder">
                        <themes jcr:primaryType="jnt:folder">
                            <default jcr:primaryType="jnt:folder">
                                <academy_icon_16_16.png j:height="16"
                                             j:width="16"
                                             jcr:mixinTypes="jmix:image"
                                             jcr:primaryType="jnt:file">
                                    <jcr:content jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                    <thumbnail jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                    <thumbnail2 jcr:mimeType="image/png" jcr:primaryType="jnt:resource"/>
                                </academy_icon_16_16.png>
                            </default>
                        </themes>
                    </files>
                    <contents jcr:primaryType="jnt:contentFolder"/>
                    <base j:theme="/modules/jahia-academy-template/templates/files/themes/default"
                  j:view="academy"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
                        <home j:icon="/modules/jahia-academy-template/templates/files/themes/default/academy_icon_16_16.png"
                     jcr:primaryType="jnt:pageTemplate">
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <j:translation_en_US jcr:language="en_US"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Academy Home Page"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Academy Home Page"/>
                            <pageContent jcr:primaryType="jnt:contentList">
                                <documentation-header fluid="true"
                                           htmlID="documentation-header"
                                           jcr:mixinTypes="bootstrap3mix:htmlID"
                                           jcr:primaryType="bootstrap3nt:container">
                                    <documentation-header jcr:primaryType="jnt:contentList">
                                        <center-content fluid="false" jcr:primaryType="bootstrap3nt:container">
                                            <center-content jcr:primaryType="jnt:contentList">
                                                <row jcr:primaryType="bootstrap3nt:row">
                                                    <home-content jcr:primaryType="bootstrap3nt:column" offset="0" span="12">
                                                        <column jcr:primaryType="jnt:contentList"/>
                                                    </home-content>
                                                </row>
                                            </center-content>
                                        </center-content>
                                    </documentation-header>
                                </documentation-header>
                            </pageContent>
                        </home>
                        <simple jcr:primaryType="jnt:pageTemplate"/>
                        <navigation jcr:primaryType="jnt:contentList">
                            <navigation-bar fluid="true"
                                  inverse="false"
                                  j:styleName="navbar navbar-default navbar-fixed-top"
                                  j:view="academy"
                                  jcr:mixinTypes="jmix:renderable"
                                  jcr:primaryType="bootstrap3nt:enhancedNavBar"
                                  option="navbar-fixed-top">
                                <j:translation_fr jcr:language="fr"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <j:translation_de jcr:language="de"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <j:translation_pt jcr:language="pt"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <j:translation_en_US jcr:language="en_US"
                                          jcr:mixinTypes="mix:title"
                                          jcr:primaryType="jnt:translation"
                                          jcr:title="Navigation-bar"/>
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <j:translation_it jcr:language="it"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <j:translation_es jcr:language="es"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Navigation-bar"/>
                                <navigation-menu j:baselineNode="home"
                                      j:maxDepth="2"
                                      j:startLevel="0"
                                      jcr:primaryType="bootstrap3nt:navBarItemMenu"
                                      position="right"/>
                                <search-form j:view="academy"
                                  jcr:mixinTypes="jmix:renderable"
                                  jcr:primaryType="bootstrap3nt:navBarItemSimpleSearchForm"
                                  position="right"
                                  result="/modules/jahia-academy-template/home"/>
                            </navigation-bar>
                        </navigation>
                        <footer jcr:primaryType="jnt:contentList">
                            <footer-1 cssClass="footer-1"
                            fluid="true"
                            jcr:mixinTypes="bootstrap3mix:cssClass"
                            jcr:primaryType="bootstrap3nt:container">
                                <container jcr:primaryType="jnt:contentList"/>
                                <footer-1 jcr:primaryType="jnt:contentList">
                                    <footer1 jcr:primaryType="jacademy:footer1"/>
                                </footer-1>
                            </footer-1>
                            <footer-2 cssClass="footer-2"
                            fluid="true"
                            jcr:mixinTypes="bootstrap3mix:cssClass"
                            jcr:primaryType="bootstrap3nt:container">
                                <footer-2 jcr:primaryType="jnt:contentList">
                                    <footer2 jcr:primaryType="jacademy:footer2"/>
                                </footer-2>
                            </footer-2>
                        </footer>
                        <product-home j:hiddenTemplate="false"
                             j:icon="/modules/jahia-academy-template/templates/files/themes/default/academy_icon_16_16.png"
                             jcr:primaryType="jnt:pageTemplate">
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <j:translation_en_US jcr:language="en_US"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Product Home"/>
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Product Home"/>
                            <pageContent jcr:primaryType="jnt:contentList">
                                <breadcrumb-container cssClass="navbar-fixed-top"
                                           fluid="true"
                                           htmlID="academy-breadcrumb"
                                           jcr:mixinTypes="bootstrap3mix:cssClass bootstrap3mix:htmlID"
                                           jcr:primaryType="bootstrap3nt:container">
                                    <breadcrumb-container jcr:primaryType="jnt:contentList">
                                        <center-container fluid="false" jcr:primaryType="bootstrap3nt:container">
                                            <center-container jcr:primaryType="jnt:contentList">
                                                <row jcr:primaryType="bootstrap3nt:row">
                                                    <column jcr:primaryType="bootstrap3nt:column" offset="0" span="4"/>
                                                    <column-1 jcr:primaryType="bootstrap3nt:column" offset="0" span="8"/>
                                                </row>
                                            </center-container>
                                        </center-container>
                                    </breadcrumb-container>
                                </breadcrumb-container>
                                <center-content-container cssClass="doc-description"
                                               fluid="false"
                                               jcr:mixinTypes="bootstrap3mix:cssClass"
                                               jcr:primaryType="bootstrap3nt:container">
                                    <center-content-container jcr:primaryType="jnt:contentList">
                                        <row jcr:primaryType="bootstrap3nt:row">
                                            <column jcr:primaryType="bootstrap3nt:column" offset="0" span="8"/>
                                            <column-1 jcr:primaryType="bootstrap3nt:column" offset="0" span="4"/>
                                        </row>
                                    </center-content-container>
                                </center-content-container>
                            </pageContent>
                        </product-home>
                        <documentation j:hiddenTemplate="false"
                              j:icon="/modules/jahia-academy-template/templates/files/themes/default/academy_icon_16_16.png"
                              jcr:primaryType="jnt:pageTemplate">
                            <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <j:translation_en_US jcr:language="en_US"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Documentation"/>
                            <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Documentation"/>
                            <pageContent jcr:primaryType="jnt:contentList">
                                <breadcrumb-container cssClass="navbar-fixed-top"
                                           fluid="true"
                                           htmlID="academy-breadcrumb"
                                           jcr:mixinTypes="bootstrap3mix:cssClass bootstrap3mix:htmlID"
                                           jcr:primaryType="bootstrap3nt:container">
                                    <breadcrumb-container jcr:primaryType="jnt:contentList">
                                        <breadcrumb-center-container fluid="false" jcr:primaryType="bootstrap3nt:container">
                                            <breadcrumb-center-container jcr:primaryType="jnt:contentList">
                                                <breadcrumb-row jcr:primaryType="bootstrap3nt:row">
                                                    <breadcrumb-column-4 cssClass="child-actions col-sm-12 col-xs-12"
                                                         jcr:mixinTypes="bootstrap3mix:cssClass"
                                                         jcr:primaryType="bootstrap3nt:column"
                                                         offset="0"
                                                         span="4"/>
                                                    <breadcrumb-column-8 cssClass="col-sm-12 col-xs-12"
                                                         jcr:mixinTypes="bootstrap3mix:cssClass"
                                                         jcr:primaryType="bootstrap3nt:column"
                                                         offset="0"
                                                         span="8"/>
                                                </breadcrumb-row>
                                            </breadcrumb-center-container>
                                        </breadcrumb-center-container>
                                    </breadcrumb-container>
                                </breadcrumb-container>
                                <document-container fluid="false"
                                         htmlID="documentation-level"
                                         jcr:mixinTypes="bootstrap3mix:htmlID"
                                         jcr:primaryType="bootstrap3nt:container">
                                    <document-container jcr:primaryType="jnt:contentList">
                                        <documentation-row jcr:primaryType="bootstrap3nt:row">
                                            <documentation-toc-column cssClass="hidden-sm hidden-xs"
                                                        htmlID="left-toc-sidebar"
                                                        jcr:mixinTypes="bootstrap3mix:cssClass bootstrap3mix:htmlID"
                                                        jcr:primaryType="bootstrap3nt:column"
                                                        offset="0"
                                                        span="4"/>
                                            <documentation-column htmlID="documentation-child-content"
                                                    jcr:mixinTypes="bootstrap3mix:htmlID"
                                                    jcr:primaryType="bootstrap3nt:column"
                                                    offset="0"
                                                    span="8"/>
                                        </documentation-row>
                                    </document-container>
                                </document-container>
                            </pageContent>
                        </documentation>
                    </base>
                    <content-template j:applyOn="jnt:content"
                              j:hiddenTemplate="true"
                              j:priority="1"
                              j:view="jahia-academy-template"
                              jcr:primaryType="jnt:contentTemplate">
                        <pagecontent jcr:primaryType="jnt:contentList">
                            <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
                        </pagecontent>
                    </content-template>
                </templates>
                
                <home j:isHomePage="true"
               j:templateName="home"
               jcr:primaryType="jnt:page">
                    <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
                </home>
            </jahia-academy-template>
    </modules>
    <sites jcr:primaryType="jnt:virtualsitesFolder">
        <systemsite jcr:primaryType="jnt:virtualsite">
            <categories jcr:primaryType="jnt:category">
                <audiences jcr:primaryType="jnt:category">
                    <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Audiences"/>
                    <marketers jcr:primaryType="jnt:category">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Marketers"/>
                    </marketers>
                    <developers jcr:primaryType="jnt:category">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Developers"/>
                    </developers>
                    <sysadmins jcr:primaryType="jnt:category">
                        <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="Sysadmins"/>
                    </sysadmins>
                </audiences>
            </categories>
        </systemsite>
    </sites>
</content>


